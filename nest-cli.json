{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/exam-system-backend/src",
  "generateOptions": {
    "spec": false
  },
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/exam-system-backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/exam-system-backend",
  "projects": {
    "analyse": {
      "type": "application",
      "root": "apps/analyse",
      "entryFile": "main",
      "sourceRoot": "apps/analyse/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analyse/tsconfig.app.json"
      }
    },
    "answer": {
      "type": "application",
      "root": "apps/answer",
      "entryFile": "main",
      "sourceRoot": "apps/answer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/answer/tsconfig.app.json"
      }
    },
    "exam": {
      "type": "application",
      "root": "apps/exam",
      "entryFile": "main",
      "sourceRoot": "apps/exam/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exam/tsconfig.app.json"
      }
    },
    "exam-system-backend": {
      "type": "application",
      "root": "apps/exam-system-backend",
      "entryFile": "main",
      "sourceRoot": "apps/exam-system-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exam-system-backend/tsconfig.app.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}
